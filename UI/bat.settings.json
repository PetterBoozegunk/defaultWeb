{
    "openUrls": [
        "http://defaultweb.local:666"
    ],
    "startBrowsers": [
        "firefox.exe"
    ],

    "cmdFiles": [{
        "fileName": "startServer",
        "commandLines": [
            "cd ..",
            "start node server.js",
            "exit"
        ]
    }, {
        "fileName": "gulp",
        "commandLines": [
            "call rimraf dist/**/*",
            "cls",
            "start gulp sync-watch",
            "gulp"
        ]
    }, {
        "fileName": "plato",
        "commandLines": [
            "call gulp platoReport",
            "cls",
            "start http://defaultweb.local:666/UI/report/index.html",
            "exit"
        ]
    }]
}